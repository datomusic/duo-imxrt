    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "./armgcc/flexspi_nor_debug/blinky.elf",
            "name": "Debug Microcontroller",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "pyocd",
            "armToolchainPath": "/opt/homebrew/bin",
            "device": "mimxrt1010",
            "targetId": "mimxrt1010",
            "interface": "swd",
            "serialNumber": "", // add J-Link serial number if having multiple attached the same time.
            "runToEntryPoint": "main",
            "svdFile": "${workspaceRoot}/MIMXRT1011.svd",
            "showDevDebugOutput": "parsed",

        }
    ]

}